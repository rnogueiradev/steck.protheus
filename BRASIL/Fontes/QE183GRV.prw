User Function QE183GRV()

Local cArea 	:= getArea()
Local aQEKArea	:= QEK->(GetArea())

If Empty(QEK->QEK_XFATEC)
	
	If !Empty(SD1->D1_XFATEC) .And. QEK->(!Eof())
		
		QEK->(RecLock('QEK',.F.))
		QEK->QEK_XFATEC := SD1->D1_XFATEC
		QEK->(MsUnLock())
		
	EndIf
	
EndIf

RestArea(aQEKArea)
RestArea(cArea)

Return()