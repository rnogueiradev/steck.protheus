User Function StVldLoc 
			
_cLocal := &(Readvar())
DbSelectArea("SX5")
DbSetOrder(1)
DbSeek(xFilial("SX5")+"90"+_cLocal)
if eof()
    MsgStop("Armazem nao existe!, Verifique !!! ")
    _lRet := .f.
else
	_lRet :=  .t.
Endif                         
                                                               
Return _lret