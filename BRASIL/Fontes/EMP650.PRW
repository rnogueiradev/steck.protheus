User Function EMP650()
Local _nI		:= 0
_cGetArea := getarea()

_cLocProd := GetMv("MV_LOCPROC")

Dbselectarea("SB1")

For _nI := 1 To Len(aCols)
	
	If DbSeek(xFilial("SB1")+aCols[_nI,1])
		If UPPER(ALLTRIM(SB1->B1_APROPRI)) <> 'D' .OR. SC2->C2_XBENEF=='S'
			aCols[_nI,3] := _cLocProd
		EndIf
	EndIf
	
Next _nI

RestArea(_cGetArea)

xEMP650()

Return
