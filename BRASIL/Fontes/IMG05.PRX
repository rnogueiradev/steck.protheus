	_cQuery1 += " WHERE CB7.D_E_L_E_T_=' ' AND CB9.D_E_L_E_T_=' ' AND C5.D_E_L_E_T_=' ' AND A1.D_E_L_E_T_=' ' AND B1.D_E_L_E_T_=' ' AND CB8.D_E_L_E_T_=' ' AND C6.D_E_L_E_T_ = ' '
