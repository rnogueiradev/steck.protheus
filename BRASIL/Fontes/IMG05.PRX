		cQueryB9 := "SELECT * FROM " +RetSqlName("CB9")
		cQueryB9 += " WHERE D_E_L_E_T_= ''" 					
		cQueryB9 += " AND CB9_VOLUME <> ''"
		cQueryB9 += " AND CB9_ORDSEP = '" +cOrdSep+"'"  	

		PLSQuery(cQueryB9, 'QUERYB9')
		DbSelectArea('QUERYB9')
		QUERYB9->(DbGoTop())

		nQtdDig := 0
		While ! QUERYB9->(EoF())

			nQtdDig += QUERYB9->CB9_QTEEMB

			QUERYB9->(DbSkip())

