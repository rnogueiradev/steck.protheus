		cQueryC9 := "SELECT * FROM " +RetSqlName("CB9")
		cQueryC9 += " WHERE D_E_L_E_T_= ''" 					
		cQueryC9 += " AND CB9_ORDSEP = '" +cOrdSep+"'" 	
		//cQueryC9 += " AND CB9_VOLUME = ''" 	

		PLSQuery(cQueryC9, 'cQueryC9')
		DbSelectArea('cQueryC9')
		cQueryC9->(DbGoTop())

