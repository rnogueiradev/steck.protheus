User Function STFINDV2()

DbSelectArea("SA1")
SA1->(DbSetorder(1))
SA1->(DbGoTop())

While SA1->(!Eof())
	Reclock("SA1",.F.)
	SA1->A1_XIDBRAD := U_STFINDV1(SA1->A1_COD)
    MsUnlock()
    SA1->(DbSkip())
End

MsgAlert("Processo Finalizado!")

Return(cRet)