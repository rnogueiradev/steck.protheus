User Function STFINDV1(cCodigo)

Local cRet := ""
Local nCodOri := 0
Local nDV := 0

nCodOri := Val(cCodigo)
nDV := nCodOri-(NoRound(nCodOri/7,0)*7)
cRet := CValToChar(nCodOri)+"-"+CValToChar(nDV)+"  "

Return(cRet)

