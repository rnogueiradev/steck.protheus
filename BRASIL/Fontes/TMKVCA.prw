User Function TMKVCA(cAtend,cCliente,cLoja,cCodCont,cCodOper)
Local nPProd	:= aPosicoes[1][2]
Local cProduto	:= aCols[n][nPProd]

If Empty(cProduto)
	Help(" ",1,"SEM PRODUT" )
	Return
Endif

SB1->(DbSetOrder(1))
If !SB1->(DbSeek(xFilial("SB1") + cProduto))
	Help(" ",1,"SEM PRODUT" )
	Return
Endif
	
U_STFSVE50(SB1->B1_COD)

Return