User Function MT130IFC()

	Local aFiltra 	:= {}
	Local _cFiltro1	:= ""
	Local _cFiltro2	:= ""

	_cFiltro1 := " .And. C1_ZSTATUS=='3'
	_cFiltro2 := " AND C1_ZSTATUS='3'

	AADD(aFiltra,_cFiltro1)
	AADD(aFiltra,_cFiltro2)

Return(aFiltra)